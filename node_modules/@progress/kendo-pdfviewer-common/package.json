{
  "name": "@progress/kendo-pdfviewer-common",
  "description": "Kendo UI TypeScript package exporting functions for PDFViewer component",
  "version": "0.3.3",
  "keywords": [
    "Kendo UI"
  ],
  "author": "Progress",
  "license": "SEE LICENSE IN LICENSE.md",
  "main": "dist/npm/main.js",
  "module": "dist/es/main.js",
  "es2015": "dist/es2015/main.js",
  "typings": "dist/npm/main.d.ts",
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=18.0.0"
  },
  "scripts": {
    "test": "jest --config jest.config.js",
    "test:ci": "jest --config jest.config.js",
    "watch-test": "jest --watch --config jest.config.js",
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider gulp start",
    "lint": "eslint .",
    "api-check": "gulp api-check",
    "build-package": "gulp build-ts-package",
    "semantic-release": "semantic-release pre && semantic-prerelease publish --public && semantic-release post"
  },
  "peerDependencies": {
    "@progress/kendo-draggable": "^3.1.0",
    "@progress/kendo-file-saver": "^1.1.1"
  },
  "dependencies": {
    "@progress/kendo-common": "^0.2.2",
    "pdfjs-dist": "^4.3.136",
    "tslib": "^2.4.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@progress/kendo-draggable": "^3.1.0",
    "@progress/kendo-file-saver": "^1.1.1",
    "@progress/kendo-typescript-tasks": "^11.0.0",
    "@progress/semantic-prerelease": "^3.0.0",
    "@types/jasmine": "~2.2.0",
    "@types/jest": "^23.3.14",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^8.56.0",
    "eslint-plugin-jest": "^27.8.0",
    "eslint-plugin-jsdoc": "^48.1.0",
    "eslint-plugin-typescript": "^0.14.0",
    "ghooks": "^1.0.3",
    "gulp": "^4.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-typescript": "^1.2.0",
    "semantic-release": "^6.3.6",
    "ts-jest": "^29.2.5",
    "typescript": "~5.4.5",
    "typescript-eslint": "^7.0.1",
    "validate-commit-msg": "^1.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/telerik/kendo-pdfviewer-common.git"
  },
  "bugs": {
    "url": "https://github.com/telerik/kendo-angular"
  },
  "release": {
    "debug": false,
    "branchTags": {
      "develop": "dev"
    },
    "fallbackTags": {
      "dev": "latest"
    },
    "analyzeCommits": "@progress/semantic-prerelease/analyzeCommits",
    "generateNotes": "@progress/semantic-prerelease/generateNotes",
    "getLastRelease": "@progress/semantic-prerelease/getLastRelease",
    "verifyConditions": "@progress/semantic-prerelease/verifyConditions",
    "verifyRelease": "@progress/semantic-prerelease/verifyRelease"
  },
  "files": [
    "dist"
  ]
}